    </tr>
  </thead>
  <tbody>
    {{#each clientes}}
    <tr>
      <td>{{nome}}</td>
      <td>{{cnpj}}</td>
      <td>

        <div class="btn-group" role="group" aria-label="Exemplo bÃ¡sico">
          <a href="/admin/clienteAberto/{{_id}}"><button class="btn btn-outline-info mr-2">Visualizar</button></a>
          <a href="/admin/editarcliente/{{_id}}"><button class="btn btn-outline-info mr-2">Editar</button></a>
          <a href="/admin/deletarCliente/{{_id}}"><button class="btn btn-outline-danger">Deletar</button></a>

